@article{Youmans2011b,
abstract = {Innovation is important for successful designs, but design fixation likely prevents designers from reaching their innovative potential. Participants in this study were 120 students, 80 of which had design backgrounds. Participants saw an example tool, and then worked alone or in small groups to design two new tools using a construction set. All participants designed two tools: one in a physical prototyping environment and one in an environment without physical prototyping. Results showed that designs were better and contained fewer fixations to the example tool when designed in the physical prototyping environment, but that groups designed better tools than individuals or nominal groups when no physical prototyping was available. These results underscore the importance of physical prototyping in design. {\textcopyright}2010 Elsevier Ltd. All rights reserved.},
