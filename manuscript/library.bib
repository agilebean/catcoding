journal = {Statistics in Medicine},
keywords = {1,directly interpretable,generalized linear models,linear regression,logistic regression,make regression coefficients more,rescaling input variables to,standardization,z -score},
pages = {2865--2873},
pmid = {19455509},
title = {{Scaling regression inputs by dividing by two standard deviations}},
volume = {27},
year = {2008}
}
