issn = {00043702},
journal = {Artificial Intelligence},
keywords = {Explainability,Explainable AI,Explanation,Interpretability,Transparency},
pages = {1--38},
title = {{Explanation in artificial intelligence: Insights from the social sciences}},
volume = {267},
year = {2019}
}
