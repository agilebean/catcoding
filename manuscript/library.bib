@article{Yannopoulos2010,
abstract = {This paper explores the role of managers' mental models in shaping the strategic direction of the firm. We propose a model which explains the role of mental models as intervening constructs between incoming information or market intelligence and the formulation of business strategy. The model explains the role of market intelligence as the source of environmental information and the role of mental models as filters and simplifying frameworks for decision making. This work discusses the functionality of mental models (cognitive structures) in managerial decision making in the context of organizational growth, strategy development and the development of organizational learning. The conceptualization is based on literature springing from theories of learning, cognition, and strategy development to propose the Process of Environmental Scanning, Interpretation and Strategic Action used by managers, and promotes the investigation of the applicability of such a concept in business networks. The paper concludes that the cognitive approach and the use of mental models can enhance our understanding of how strategic decisions become a function of mental models used by managers.},
author = {Yannopoulos, Peter and Daskou, Sofia},
