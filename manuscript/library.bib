keywords = {creativity,personality},
mendeley-tags = {creativity,personality},
number = {3},
pages = {e91669},
pmid = {24670894},
title = {{Development and validation of a new multidimensional measure of inspiration: associations with risk for bipolar disorder.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3966762{\&}tool=pmcentrez{\&}rendertype=abstract},
