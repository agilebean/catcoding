doi = {10.1177/0961463X11414296},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2011 - Stolarski, Bitner, Zimbardo - Time perspective, emotional intelligence and discounting of delayed awards.pdf:pdf},
