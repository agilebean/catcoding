journal = {Symmetry},
keywords = {EEG,Emotion,executive functions,hemispheric asymmetry,lateralisation},
number = {8},
title = {{Effects of emotional valence on hemispheric asymmetries in response inhibition}},
volume = {9},
year = {2017}
