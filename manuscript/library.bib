file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2018 - M{\"{a}}ntyl{\"{a}}, Graziotin, Kuutila - The Evolution of Sentiment Analysis.pdf:pdf},
isbn = {1612.01556},
issn = {15740137},
journal = {Computer Review},
keywords = {bibliometric study,latent dirichlet allocation,literature review,opinion mining,qualitative analysis,sentiment analysis,text mining,topic modelling},
number = {February},
