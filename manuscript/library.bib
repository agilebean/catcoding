file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2010 - Fumoto et al. - Ventral prefrontal cortex and serotonergic system activation during pedaling exercise induces negative mood impro.pdf:pdf},
isbn = {1872-7549 (Electronic){\$}\backslash{\$}n0166-4328 (Linking)},
issn = {01664328},
journal = {Behavioural Brain Research},
keywords = {Cerebral blood flow,Electroencephalography,Exercise,Human,Mood state,Near-infrared spectroscopy,Prefrontal cortex,Serotonin},
number = {1},
pages = {1--9},
