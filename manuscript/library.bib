journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Adolescent,Adult,Beauty,Choice Behavior,Computer Graphics,Face,Female,Humans,Immunocompetence,Male,Perception,Sexual Behavior,Social Dominance,Testosterone,Testosterone: blood,attractive},
mendeley-tags = {attractive},
month = {nov},
