journal = {Journal of Behavioral Addictions},
keywords = {addiction,mobile phone,predictors,smartphone,students},
number = {4},
pages = {299--307},
pmid = {26690625},
title = {{Smartphone use and smartphone addiction among young people in Switzerland}},
url = {http://www.akademiai.com/doi/abs/10.1556/2006.4.2015.037},
