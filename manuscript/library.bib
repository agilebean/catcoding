file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2017 - Ulyanov, Vedaldi, Lempitsky - Improved Texture Networks Maximizing Quality and Diversity in Feed-forward Stylization and Texture.pdf:pdf},
isbn = {978-1-5386-0457-1},
title = {{Improved Texture Networks: Maximizing Quality and Diversity in Feed-forward Stylization and Texture Synthesis}},
url = {http://arxiv.org/abs/1701.02096},
