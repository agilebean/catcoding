@article{Lim2007,
abstract = {Although there has been a drastic increase in the research of aesthet- ics of interaction, we still lack well-defined practical knowledge of how to de- sign aesthetic interactions. In order to develop such knowledge, we adapt three important ways of thinking in designing interactions influenced by traditional design disciplines, namely, 1) understanding what it is that is designed—i.e. in- teraction, 2) knowing what is possible to be manipulated when designing inter- actions—i.e. attributes of interaction, and 3) mastering how to manipulate the attributes to shape the interactions. We explain our approach by arguing from the somaesthetic perspective. We propose the concept of interaction gestalt, as a way to achieve those three ways of thinking in design. We then propose a set of interaction gestalt attributes that can be used in designing aesthetic interac- tions. We end with a discussion of the implications and benefits of this ap- proach in interaction design.},
author = {Lim, Youn-kyung and Stolterman, Erik and Jung, Heekyoung and Donaldson, Justin},
doi = {10.1145/1314161.1314183},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2007 - Lim et al. - Interaction gestalt and the design of aesthetic interactions.pdf:pdf},
isbn = {9781595939425},
journal = {Proceedings of the 2007 conference on Designing pleasurable products and interfaces - DPPI '07},
keywords = {aesthetic interactions,aesthetics of interaction,in-,user experience},
number = {August},
