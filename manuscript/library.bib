isbn = {0898-929X (Print){\$}\backslash{\$}n0898-929X (Linking)},
issn = {0898-929X},
journal = {Journal of Cognitive Neuroscience},
number = {1},
pages = {42--58},
pmid = {17214562},
title = {{The Neural Substrate of Human Empathy: Effects of Perspective-taking and Cognitive Appraisal}},
