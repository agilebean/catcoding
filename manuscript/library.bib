journal = {Administrative Science Quarterly},
number = {4},
pages = {716--749},
pmid = {61521},
title = {{Brainstorming Groups in Context: Effectiveness in a Product Design Firm}},
url = {http://www.jstor.org/stable/2393655{\%}5Cnhttp://www.jstor.org/stable/pdfplus/2393655.pdf},
