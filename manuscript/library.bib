journal = {IEEE Transactions on Audio, Speech, and Language Processing},
keywords = {academic research,academic software,academics,bibliography,digital library,emotion,library management,library software,machine learning,reference software,research paper,research tool,researcher},
mendeley-tags = {emotion,machine learning},
