arxivId = {arXiv:1011.1669v3},
author = {Steyerberg, Ewout W. and Jr, Frank E. Harrell and Borsboom, Gerard J. J. M. and Eijkemans, M. J. C. (Ren{\'{e}}) (Ren{\'{e}}) and Vergouwe, Yvonne and Habbema, J. Dik F.},
