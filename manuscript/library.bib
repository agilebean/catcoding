doi = {10.1037/a0035036},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2014 - Williams et al. - Mindfulness-based cognitive therapy for preventing relapse in recurrent depression A randomized dismantling tri.pdf:pdf},
isbn = {1939-2117{\$}\backslash{\$}r0022-006X},
issn = {19392117},
journal = {Journal of Consulting and Clinical Psychology},
