doi = {10.1007/BF02017162},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/1985 - Friedman, Ken Barron, Deirdre Ferlazzo, Silvana Ivanka, Tania Melles, Gavin Yuille - Design research journal ranking study prelim.pdf:pdf},
issn = {0138-9130},
journal = {Scientometrics},
