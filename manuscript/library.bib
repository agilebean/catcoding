isbn = {00131954 (ISSN)},
issn = {00131954},
journal = {Educational Studies in Mathematics},
keywords = {Binomial,Cognition,College,Combinatorial analysis,Computer,Gesture,Media,Model,Multimodality,Outcome distribution,ProbLab,Probability,Problem solving,Reasoning,Semiotics,Simulation,embodied},
mendeley-tags = {embodied},
