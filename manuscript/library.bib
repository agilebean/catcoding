keywords = {2AFC,Crowdsourcing,Evaluation,Evaluation methods,Image comparison,Image evaluation,Tool,Visual analytics,Visualization,Visualization evaluation},
number = {4},
pages = {225--234},
publisher = {Elsevier B.V.},
title = {{Versus—A tool for evaluating visualizations and image quality using a 2AFC methodology}},
url = {https://doi.org/10.1016/j.visinf.2018.12.003},
volume = {2},
