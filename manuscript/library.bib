}
@article{Lim2001,
abstract = {Implicit theories of creativity have been the subject of increased{\$}\backslash{\$}nresearch interest in recent years. These investigations are motivated by{\$}\backslash{\$}nthe observation that an individual's creative activities are guided by{\$}\backslash{\$}npersonal definitions rather than professional theories, which may be{\$}\backslash{\$}nvery different. Cross-cultural studies of implicit creativity theories{\$}\backslash{\$}nare rare, yet they can add significantly to our knowledge of how{\$}\backslash{\$}ncreativity is viewed across cultures. The nature of 428 Koreans'{\$}\backslash{\$}nimplicit creativity theories were identified in a prestudy, and the{\$}\backslash{\$}nstructure of 478 Koreans' ratings of the indicated behaviors was{\$}\backslash{\$}nanalyzed in Experiment 1. In Experiment 2, 211 participants evaluated{\$}\backslash{\$}nthe creativity of 44 hypothetical profiles based on the results of the{\$}\backslash{\$}nfirst experiment. Results provide evidence that Korean conceptions of{\$}\backslash{\$}ncreativity are similar to Western conceptions, although Koreans may{\$}\backslash{\$}nemphasize negative behaviors and personality characteristics (e.g.,{\$}\backslash{\$}ndeviance) to a greater degree. When asked to use their implicit theories{\$}\backslash{\$}nto evaluate the creativity of hypothetical profiles, Korean adults{\$}\backslash{\$}nstrongly emphasized specific cognitive, personality, and motivational{\$}\backslash{\$}naspects of creativity over noncognitive aspects (e.g., perseverance,{\$}\backslash{\$}nindependence).},
