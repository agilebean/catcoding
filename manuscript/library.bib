title = {{Bitcoin Spread Prediction Using Social and Web Search Media}},
url = {https://markets.blockchain.info/{\%}0Ahttps://pdfs.semanticscholar.org/1345/a50edee28418900e2c1a4292ccc51138e1eb.pdf},
year = {2015}
}
