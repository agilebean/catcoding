journal = {Personality and Social Psychology Review},
keywords = {Automatic/implicit processes,Emotion,Social cognition,self-regulation},
mendeley-tags = {self-regulation},
number = {2},
pages = {167--203},
pmid = {18453461},
title = {{How Emotion Shapes Behavior: Feedback, Anticipation, and Reflection, Rather Than Direct Causation}},
