@article{Costafreda2008,
abstract = {Although amygdala activity has been purported to be modulated by affective and non-affective factors, considerable controversy remains on its precise functional nature. We conducted a meta-analysis of 385 functional neuroimaging studies of emotional processing, examining the effects of experimental characteristics on the probability of detecting amygdala activity. All emotional stimuli were associated with higher probability of amygdala activity than neutral stimuli. Comparable effects were observed for most negative and positive emotions, however there was a higher probability of activation for fear and disgust relative to happiness. The level of attentional processing affected amygdala activity, as passive processing was associated with a higher probability of activation than active task instructions. Gustatory-olfactory and visual stimulus modalities increased the probability of activation relative to internal stimuli. Aversive learning increased the probability of amygdala activation as well. There was some evidence of hemispheric specialization with a relative left-lateralization for stimuli containing language and a relative right-lateralization for masked stimuli. Methodological variables, such as type of analysis and magnet strength, were also independent predictors of amygdala activation. ?? 2007 Elsevier B.V. All rights reserved.},
author = {Costafreda, Sergi G. and Brammer, Michael J. and David, Anthony S. and Fu, Cynthia H Y},
doi = {10.1016/j.brainresrev.2007.10.012},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2008 - Costafreda et al. - Predictors of amygdala activation during the processing of emotional stimuli A meta-analysis of 385 PET and f.pdf:pdf},
isbn = {0165-0173 (Print)},
issn = {01650173},
journal = {Brain Research Reviews},
keywords = {Amygdala,Emotion,Meta-analysis,Neuroimaging,PET,fMRI},
number = {1},
pages = {57--70},
pmid = {18076995},
title = {{Predictors of amygdala activation during the processing of emotional stimuli: A meta-analysis of 385 PET and fMRI studies}},
volume = {58},
year = {2008}
}
