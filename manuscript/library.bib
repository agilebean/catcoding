@article{Hinsz1997,
annote = {a{\{}{\_}{\}}feedback

Feedback is also important for group performance
(Barr {\{}{\&}{\}}amp; Conlon, 1994; Nadler, 1979; Robinson {\{}{\&}{\}}amp; Weldon,
1993; Tindale, 1989; Tindale, Kulik, {\{}{\&}{\}}amp; Scott, 1991), although
the supporting research is somewhat sparse and inconsistent

four aspects of feedback in groups processing information: 
(a) a comparison of the use of feedback by groups and individuals,
(b) the impact of feedback from other group members, 
(c) the distinction between process and outcome feedback, and 
(d) the effect of feedback on group member attributions.

Groups seem better able to use feedback than individuals.

Group discussion may work much like feedback
in the studies above, in that group members get more accurate
representations of how others will act, although increased social
identity and intermember commitments also seem important
(Dawes et al., 1988; Kerr {\{}{\&}{\}}amp; Kaufman-Gilliland, 1994). 

Group discussion can also serve as a type of feedback to group members
(Tindale, 1989). Discussion may also allow members to
realize that other members made choices different from their
own (Hastie {\{}{\&}{\}}amp; Pennington, 1991). This type of social feedback
can reduce confidence (Sniezek, 1992). Taken together, these
studies suggest that group discussion inherently provides internal
feedback to group members, regardless of whether other
external feedback is present.
=
Feedback in groups also affects group member attributions.
Individual-level research has consistently shown that people
make self-serving attributions by attributing success to themselves
and failure to the situation (Ross, 1981). Although group
members also show this tendency (see Leary {\{}{\&}{\}}amp; Forsyth, 1987),
they often attribute responsibility for positive group outcomes
to the group rather than to themselves individually (Forsyth {\{}{\&}{\}}amp;
Schlenker, 1977; Tindale et al., 1991). In addition, group members
use group-level feedback in much the same way as individual-
level feedback when making attributions, especially when
individual feedback is unavailable or negative (Tindale et al.,
1991). Thus, group feedback may change the self-attributional
focus from the individual to the group, which is consistent with
the notions of social categorization theory (Turner {\{}{\&}{\}}amp; Oakes,
1989).},
author = {Hinsz, Verlin B and Tindale, R Scott},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/1997 - Hinsz, Tindale - The Emerging Conceptualization of Groups as Information Processors.pdf:pdf},
journal = {Psychological Bulletin},
keywords = {feedback},
mendeley-tags = {feedback},
pages = {43--64},
title = {{The Emerging Conceptualization of Groups as Information Processors}},
volume = {121},
year = {1997}
}
@article{Fokkinga2013,
abstract = {This paper demonstrates how designers can enrich user experiences by purposefully involving negative emotions in user-product interaction. This approach is derived from a framework of rich experience, which explains how and under what circumstances negative emotions make a product experience richer and enjoyable. The approach consists of three steps, where the designer decides 1) which negative emotion is most appropriate for the user context; 2) how and when this emotion is best elicited; and 3) which protective frame is most appropriate to use and in what way it is applied to the product concept. Ten experience qualities were developed that offer prefabricated combinations of these steps, which are intended to lower the threshold of using the approach. The steps for these qualities are described, and each is briefly discussed. Lastly, the applicability of the approach in design is demonstrated by showing six examples of how the qualities have been used to generate concrete product concepts. Reflecting on the approach, we conclude that negative emotions are a viable and interesting starting point for creating emotionally rich product experiences.},
author = {Fokkinga, Steven F. and Desmet, Pieter M A},
file = {:Users/chaehan/Google Drive/04 Publishing/10 Literature Archive/2013 - Fokkinga, Desmet - Ten ways to design for disgust, sadness, and other enjoyments A Design Approach to Enrich Product Experiences.pdf:pdf},
issn = {19913761},
journal = {International Journal of Design},
keywords = {Design approach,Emotional design,Negative emotion,Rich experience,emotion,paper5{\_}designdimensions},
