journal = {LREC 2018 - 11th International Conference on Language Resources and Evaluation},
keywords = {Convolutional Neural Network,Deep learning,Machine learning,Medical blog,Medical sentiment},
pages = {2790--2797},
title = {{Medical sentiment analysis using social media: Towards building a patient assisted system}},
year = {2019}
}
